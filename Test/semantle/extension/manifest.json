{
  "name": "Semantle ",
  "version": "1.0.0",
  "description": "Gives a DUO Semantle",
  "manifest_version": 3,
  "author": "mrblacklicorice",
  "action": {
    "default_popup": "index.html",
    "default_title": "Semantle"
  },
  "permissions": [
    "storage",
    "activeTab",
    "declarativeContent"
  ],
  "content_scripts": [
    {
      "matches": [
        "<all_urls>"
      ],
      "js": [
        "content.js"
      ]
    }
  ]
}